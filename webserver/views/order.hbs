<p>Order: {{data.id}}</p>

<p>name: {{data.customer_name}}</p>

{{#each data.lineItems}}
<ul>
  <li>Item: {{product.description}}</li>
  <li>Price: {{product.price}} </li>
  <li>Quantity: {{quantity}} </li>
  <li>Subtotal: {{subTotal}}</li>
</ul>
{{/each}}

<hr>

<form action="/orders?_method=DELETE" method="POST">
  <input type="submit" value="Cancel Order"></input>
</form>
